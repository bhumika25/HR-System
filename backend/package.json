{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma migrate reset",
    "db:studio": "npx prisma studio",
    "docker:up": "cd docker && docker-compose up -d",
    "docker:down": "cd docker && docker-compose down",
    "docker:logs": "cd docker && docker-compose logs -f",
    "docker:restart": "npm run docker:down && npm run docker:up",
    "setup": "npm run docker:up && sleep 10 && npm run db:generate && npm run db:migrate && npm run db:seed"
  },
